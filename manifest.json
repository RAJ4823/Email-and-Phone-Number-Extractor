{
    "name": "Emails & Phone Numbers Extractor",
    "description": "Chrome Extension to extract emails or phone numbers from web page",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["scripting", "activeTab"],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
       "16": "/images/icon-16.png",
       "32": "/images/icon-32.png",
       "64": "/images/icon-64.png",
       "128": "/images/icon-128.png"
    }

}